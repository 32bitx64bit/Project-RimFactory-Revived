<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--==================================== DataDisk ==============================-->
  <RecipeDef>
    <defName>MakeDataDisk</defName>
    <label>make data disk</label>
    <description>Make a roll of magnetic data tape.</description>
    <jobString>Making data disk.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>5000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <RimFactory_Data_Disk>1</RimFactory_Data_Disk>
    </products>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>TableMachining</li>
      <li>TableRoboticMachining</li>
    </recipeUsers>
    <researchPrerequisite>MagneticTape</researchPrerequisite>
  </RecipeDef>


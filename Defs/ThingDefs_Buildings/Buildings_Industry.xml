        <workSpeedBaseFactor>1</workSpeedBaseFactor>
        <doEffect>false</doEffect>
        <drawStatus>true</drawStatus>
        <workingGraphicData>
          <graphicClass>Graphic_Single</graphicClass>
          <texPath>Industry/smelter_t0</texPath>

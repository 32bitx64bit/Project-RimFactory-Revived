<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Alpha Biomes</li>
    </mods>
    <!-- Mark III deep quarry shouldn't yield Alpha Biomes chunks -->
    <match Class="PatchOperationAdd">
      <xpath>/Defs/ThingDef[starts-with(defName, "PRF_DeepQuarry_MarkIII")]/modExtensions/li[@Class="ProjectRimFactory.Common.ModExtension_Miner"]/excludeOres</xpath>
      <value>
        <li>AB_AlcyoniteChunk</li>
        <li>GU_ChunkRoseQuartz</li>
        <li>AB_ChunkCragstone</li>
        <li>AB_ChunkObsidian</li>
        <li>AB_ChunkMudstone</li>
        <li>AB_ChunkSlimeStone</li>
      </value>
    </match>
  </Operation>
</Patch>
